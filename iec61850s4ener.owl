    <owl:ObjectProperty rdf:about="https://saref.etsi.org/saref4ener/hasDevice">
        <rdfs:comment xml:lang="en">A relationship between a load control event (used to send overload warning severity level and related load control commands) and the device interested by this event.</rdfs:comment>
        <rdfs:label xml:lang="en">has device</rdfs:label>
    </owl:ObjectProperty>
    


