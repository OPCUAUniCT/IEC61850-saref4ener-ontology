        <rdfs:label xml:lang="en">device name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/saref4ener/eventID -->

    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/saref4ener/eventID">
